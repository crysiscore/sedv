<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="507.0" prefWidth="586.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.BuscaProdutosController">
   <children>
      <TableView fx:id="tableviewProdutos" layoutX="101.0" layoutY="108.0" prefHeight="256.0" prefWidth="353.0">
        <columns>
          <TableColumn fx:id="columnCodigoProduto" prefWidth="132.0" text="Codigo" />
          <TableColumn fx:id="columnNomeProduto" prefWidth="244.0" text="Nome" />
        </columns>
      </TableView>
      <TextField fx:id="textfieldProcurar" layoutX="101.0" layoutY="63.0" prefHeight="25.0" prefWidth="353.0" promptText="Introduza o Nome ou Codigo para  procurar o produto" />
      <Button fx:id="buttonAdicionarProduto" layoutX="101.0" layoutY="418.0" mnemonicParsing="false" onAction="#handleMenuItemStockAdicionarProduto" prefHeight="25.0" prefWidth="167.0" text="Adicionar Produto" />
      <Label fx:id="labelUsuario" layoutX="426.0" layoutY="14.0" />
      <Label fx:id="labelCodProduto" layoutX="426.0" layoutY="422.0" />
      <Button fx:id="buttonCancelar" layoutX="278.0" layoutY="418.0" mnemonicParsing="false" onAction="#CancelButtonOnAction" prefHeight="25.0" prefWidth="167.0" text="Cancelar" />
   </children>
</AnchorPane>
